<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />

</head>
<body>

    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <a class="navbar-brand" href="#">OtoSoft</a>
            
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Herbs</a>
                    </li>
                     <li class="nav-item">
                        <a class="nav-link" asp-controller="Complaint" asp-action="Index">Complaints </a>
                    </li>
                </ul>
            </div>
            
        </nav>
    </header>
    <main class="container">
        <section>
            @RenderBody()
        </section>
    </main>
</body>
</html>
